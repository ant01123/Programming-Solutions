#include <bits/stdc++.h>
using namespace std;

int main() {
	for(int i=0; i<10; ++i) {
		cout<<"Enter any number other than "<<i<<": ";
		int userInput;
		cin>>userInput;
		if(userInput==i) {
			cout<<"Hey! you weren't supposed to enter "<<i<<"!";
			return 0;
		}
	}
	cout<<"Wow, you're more patient than I am, you win.";
	return 0;
}
